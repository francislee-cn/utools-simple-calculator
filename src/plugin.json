{
  "version": "1.0.4",
  "pluginName": "简易计算器",
  "description": "简便快捷的计算器",
  "author": "Francis_Lee",
  "homepage": "https://github.com/francislee-cn/utools-simple-calculator",
  "preload": "preload.js",
  "logo": "logo.png",
  "features": [
    {
      "code": "calculator",
      "explain": "计算算式",
      "cmds": [
        "calculator",
        "简易计算器",
        {
          "type": "regex",
          "label": "简易计算器",
          "match": "/^(?:\\(*-?\\d+(\\.\\d+)?\\)* ?[+\\-*/%] ?)+\\(*-?\\d+(\\.\\d+)?\\)*$/"
        }
      ]
    }
  ]
}
